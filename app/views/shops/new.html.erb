<div class="shops-new-wrapper">
  <div class="container">
    <div class="row">
      <div class="col-md-offset-4 col-md-4 shops-new-container">
        <h3 class="text-center text-white">店舗登録して下さい</h3>

          <%= form_for @shop do |f| %>
            <%= f.label :"店舗住所" %><br>
            <% shop_list = @shops.map{|shop|[prefecure_id]} %>
          <%= f.select :prefecure_id, shop_list %>

          <div class="form-group">
            <%= f.label :name, class: 'text-white' %>
            <%= f.text_field :name, class: 'form-control' %>
          </div>

          <div class="form-group">
            <%= f.label :email, class: 'text-white' %>
            <%= f.text_field :email, class: 'form-control' %>
          </div>



          <%= f.submit "店舗登録", class: 'btn-block btn-white' %>
        <% end %>

      </div>
    </div>
  </div>
</div>
